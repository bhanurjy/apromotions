@{
    ViewBag.Title = "PostRequirements";
    Layout = "~/Views/Shared/login_Layout.cshtml";
}
<!-- Contact form element -->
<head>
    <title>Upload Art work</title>
</head>
<body ng-controller="PostRequirementsCtrl">
    <form name="PostRequirementsform" autocomplete="off">
        <div class="contactForm">
            <div class="row ap-signmain">
                <div class="col-md-1 col-sm-1"></div>
                <div class="col-md-10 col-sm-10">
